{
  "name": "@travi/hapi-react-router",
  "license": "MIT",
  "description": "hapi route to delegate routing for html content to react-router",
  "version": "0.0.0-semantically-released",
  "main": "lib/plugin.cjs.js",
  "module": "lib/plugin.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint '**/*.md'",
    "tests:unit:no-coverage": "mocha --recursive test/unit",
    "coverage": "nyc run-s tests:unit:no-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit": "run-s coverage",
    "test:integration": "run-s test:integration:base -- --tags 'not @wip'",
    "test:integration:base": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:debug": "DEBUG=test run-s tests:integration",
    "test:integration:wip": "run-s test:integration:base -- --tags @wip",
    "test": "run-s lint:* test:*",
    "start": "cross-env BABEL_ENV=example babel-node ./example/app.js",
    "build": "rollup -c",
    "preversion": "npm test",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/hapi-react-router.git"
  },
  "keywords": [
    "hapi",
    "react",
    "react-router",
    "server-side render",
    "ssr",
    "redux",
    "redial"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "bugs": {
    "url": "https://github.com/travi/hapi-react-router/issues"
  },
  "homepage": "https://github.com/travi/hapi-react-router#readme",
  "devDependencies": {
    "@babel/node": "^7.6.1",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/register": "^7.6.0",
    "@hapi/boom": "^8.0.1",
    "@hapi/glue": "^6.1.1",
    "@travi/any": "^2.0.3",
    "@travi/babel-preset": "^3.0.0",
    "@travi/eslint-config": "1.0.25",
    "@travi/eslint-config-cucumber": "1.0.3",
    "@travi/eslint-config-mocha": "1.0.5",
    "@travi/eslint-config-react": "1.0.20",
    "@travi/hapi-html-request-router": "^3.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov": "^3.5.0",
    "commitlint-config-travi": "^1.3.3",
    "cross-env": "^6.0.0",
    "cucumber": "^6.0.2",
    "cz-conventional-changelog": "^3.0.2",
    "globstar": "1.0.0",
    "good": "^8.1.2",
    "good-console": "^8.0.0",
    "good-squeeze": "5.1.0",
    "husky": "^3.0.4",
    "markdownlint-cli": "^0.19.0",
    "material-ui": "0.20.2",
    "mocha": "^6.2.0",
    "mustache": "^3.0.2",
    "npm-run-all": "4.1.5",
    "nyc": "^14.1.1",
    "prop-types": "15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^6.0.0",
    "react-router": "^3.2.1",
    "redial": "0.5.0",
    "redux": "^4.0.4",
    "rollup": "^1.20.2",
    "rollup-plugin-babel": "^4.3.3",
    "sinon": "^7.4.1",
    "vision": "^5.4.4"
  },
  "peerDependencies": {
    "@hapi/boom": "^8.0.0",
    "@hapi/hapi": "^18.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.0.5",
    "redial": "^0.5.0"
  },
  "optionalDependencies": {
    "@travi/hapi-html-request-router": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "http-status-codes": "^1.3.2"
  }
}
