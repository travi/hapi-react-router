{
  "name": "@travi/hapi-react-router",
  "license": "MIT",
  "description": "hapi route to delegate routing for html content to react-router",
  "version": "0.0.0-semantically-released",
  "main": "lib/plugin.cjs.js",
  "module": "lib/plugin.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint '**/*.md'",
    "tests:unit:no-coverage": "mocha --recursive test/unit",
    "coverage": "nyc run-s tests:unit:no-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit": "run-s coverage",
    "test:integration": "run-s test:integration:base -- --tags 'not @wip'",
    "test:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:debug": "DEBUG=test run-s tests:integration",
    "test:integration:wip": "run-s test:integration:base -- --tags @wip",
    "test": "run-s lint:* test:*",
    "start": "cross-env BABEL_ENV=example babel-node ./example/app.js",
    "build": "rollup -c",
    "preversion": "npm test",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "prepublishOnly": "npm run build",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/hapi-react-router.git"
  },
  "keywords": [
    "hapi",
    "react",
    "react-router",
    "server-side render",
    "ssr",
    "redux",
    "redial"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "bugs": {
    "url": "https://github.com/travi/hapi-react-router/issues"
  },
  "homepage": "https://github.com/travi/hapi-react-router#readme",
  "devDependencies": {
    "@travi/any": "1.7.5",
    "@travi/eslint-config-travi": "1.6.14",
    "@travi/hapi-html-request-router": "^2.0.0",
    "babel-cli": "6.26.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-travi": "^1.3.1",
    "babel-register": "6.26.0",
    "boom": "^6.0.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.0.4",
    "cross-env": "5.1.6",
    "cucumber": "^4.2.1",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "glue": "5.0.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "5.1.0",
    "greenkeeper-lockfile": "1.15.1",
    "hapi": "^17.5.1",
    "husky": "^1.0.0-rc.6",
    "markdownlint-cli": "0.13.0",
    "material-ui": "0.20.2",
    "mocha": "5.2.0",
    "mustache": "^2.3.0",
    "npm-run-all": "4.1.3",
    "nyc": "^12.0.2",
    "prop-types": "15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "5.0.7",
    "react-router": "^3.2.1",
    "redial": "0.5.0",
    "redux": "4.0.0",
    "rollup": "0.65.0",
    "rollup-plugin-babel": "3.0.7",
    "sinon": "^6.0.0",
    "validate-commit-msg": "2.14.0",
    "vision": "^5.3.2"
  },
  "peerDependencies": {
    "boom": "^6.0.0",
    "hapi": "^17.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.0.5",
    "redial": "^0.5.0"
  },
  "optionalDependencies": {
    "@travi/hapi-html-request-router": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "http-status-codes": "1.3.0"
  }
}
