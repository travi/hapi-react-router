{
  "name": "@travi/hapi-react-router",
  "license": "MIT",
  "description": "hapi route to delegate routing for html content to react-router",
  "version": "0.0.0-semantically-released",
  "main": "lib/plugin.cjs.js",
  "module": "lib/plugin.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint '**/*.md'",
    "tests:unit:no-coverage": "mocha --recursive test/unit",
    "coverage": "nyc run-s tests:unit:no-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "tests:unit": "run-s coverage",
    "tests:integration": "cucumber.js test/integration --compiler js:babel-register --tags 'not @wip'",
    "tests:integration:debug": "DEBUG=test run-s tests:integration",
    "tests:integration:wip": "cucumber.js test/integration --compiler js:babel-register --tags @wip",
    "test": "run-s lint:* tests:*",
    "start": "cross-env BABEL_ENV=example babel-node ./example/app.js",
    "build": "rollup -c",
    "preversion": "npm test",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "presemantic-release": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/hapi-react-router.git"
  },
  "keywords": [
    "hapi",
    "react",
    "react-router",
    "server-side render",
    "ssr",
    "redux",
    "redial"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "bugs": {
    "url": "https://github.com/travi/hapi-react-router/issues"
  },
  "homepage": "https://github.com/travi/hapi-react-router#readme",
  "devDependencies": {
    "@travi/any": "1.4.3",
    "@travi/eslint-config-travi": "1.5.12",
    "@travi/hapi-html-request-router": "1.1.0",
    "babel-cli": "6.26.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "boom": "6.0.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "2.3.1",
    "cross-env": "5.1.0",
    "cucumber": "3.0.6",
    "cz-conventional-changelog": "2.0.0",
    "globstar": "1.0.0",
    "glue": "4.2.0",
    "good": "7.3.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.2",
    "hapi": "16.6.2",
    "hapi-mustache": "0.0.1",
    "husky": "0.15.0-beta.2",
    "markdownlint-cli": "0.3.1",
    "material-ui": "0.19.4",
    "mocha": "4.0.1",
    "npm-run-all": "4.1.1",
    "nyc": "11.2.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-redux": "5.0.6",
    "react-router": "3.0.5",
    "react-tap-event-plugin": "3.0.2",
    "redial": "0.5.0",
    "redux": "3.7.2",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "semantic-release": "8.2.0",
    "sinon": "4.0.1",
    "validate-commit-msg": "2.14.0",
    "vision": "4.1.1",
    "visionary": "6.0.2"
  },
  "peerDependencies": {
    "boom": "^5.1.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-router": "^3.0.5",
    "redial": "^0.5.0"
  },
  "optionalDependencies": {
    "@travi/hapi-html-request-router": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "http-status-codes": "1.3.0"
  }
}
