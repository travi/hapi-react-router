{
  "name": "@travi/hapi-react-router",
  "license": "MIT",
  "description": "hapi route to delegate routing for html content to react-router",
  "version": "0.3.0",
  "main": "lib/plugin.cjs.js",
  "module": "lib/plugin.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* tests:*",
    "start": "cross-env BABEL_ENV=example babel-node ./example/app.js",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/hapi-react-router.git"
  },
  "keywords": [
    "hapi",
    "react",
    "react-router",
    "server-side render",
    "ssr",
    "redux"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "bugs": {
    "url": "https://github.com/travi/hapi-react-router/issues"
  },
  "homepage": "https://github.com/travi/hapi-react-router#readme",
  "devDependencies": {
    "@travi/any": "0.7.0",
    "@travi/eslint-config-travi": "0.3.0",
    "@travi/hapi-html-request-router": "0.1.0",
    "babel-cli": "6.18.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.17.0",
    "boom": "4.2.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.10.2",
    "glue": "4.0.0",
    "good": "7.0.2",
    "good-console": "6.3.1",
    "good-squeeze": "5.0.1",
    "greenkeeper-postpublish": "1.0.1",
    "hapi-mustache": "0.0.1",
    "husky": "0.11.9",
    "material-ui": "0.16.4",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "3.0.0",
    "redial": "0.5.0",
    "redux": "3.6.0",
    "rollup": "0.36.3",
    "rollup-plugin-babel": "2.6.1",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "validate-commit-msg": "2.8.2",
    "vision": "4.1.0",
    "visionary": "6.0.2"
  },
  "peerDependencies": {
    "boom": "^4.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-router": "^3.0.0",
    "redial": "^0.5.0"
  },
  "optionalDependencies": {
    "@travi/hapi-html-request-router": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
